CREATE or replace SECURITY INTEGRATION SALESFORCE_OAUTH_INTEGRATION
  TYPE = OAUTH
  ENABLED = TRUE
  OAUTH_CLIENT = CUSTOM
  OAUTH_CLIENT_TYPE = 'CONFIDENTIAL'
  OAUTH_REDIRECT_URI = 'https://kasetti2-dev-ed.develop.my.salesforce.com/services/authcallback/snowflake_salesforce'
  OAUTH_ISSUE_REFRESH_TOKENS = TRUE
  OAUTH_ALLOW_NON_TLS_REDIRECT_URI = FALSE
  OAUTH_ENFORCE_PKCE = FALSE
  BLOCKED_ROLES_LIST = ()
  OAUTH_USE_SECONDARY_ROLES = 'IMPLICIT';
//OAUTH_REDIRECT_URI ----use according to salesforce org
  -- Get new configuration
DESCRIBE SECURITY INTEGRATION SALESFORCE_OAUTH_INTEGRATION;

-- Get new client secret
SELECT SYSTEM$SHOW_OAUTH_CLIENT_SECRETS('SALESFORCE_OAUTH_INTEGRATION');

